{"ast":null,"code":"export default {\n  name: \"MainContent\",\n  data() {\n    return {\n      tableData: [],\n      pageSize: 10,\n      pageNum: 1,\n      total: 0,\n      data: '',\n      name: '' //refer v-model里的name\n    };\n  },\n\n  methods: {\n    handleSizeChange(val) {\n      console.log(`${val} items per page`);\n      this.pageNum = 1;\n      this.pageSize = val;\n      this.loadPost();\n    },\n    handleCurrentChange(val) {\n      console.log(`current page: ${val}`);\n      this.pageNum = val;\n      this.loadPost();\n    },\n    loadGet() {\n      this.$axios.get(this.$httpUrl + '/user/list').then(res => res.data).then(res => {\n        console.log(res);\n      });\n    },\n    loadPost() {\n      this.$axios.post(this.$httpUrl + '/user/listPageEn', {\n        pageSize: this.pageSize,\n        pageNum: this.pageNum,\n        param: {\n          name: this.name //refer data里的name\n        }\n      }).then(res => res.data).then(res => {\n        console.log(res);\n        if (res.code == 200) {\n          this.tableData = res.data;\n          this.total = res.total;\n        } else {\n          alert(\"Resource Get Failed\");\n        }\n      });\n    }\n  },\n  beforeMount() {\n    this.loadPost();\n  }\n};","map":{"version":3,"mappings":"AA0DA;EACAA;EACAC;IACA;MACAC;MACAC;MACAC;MACAC;MACAJ;MACAD;IACA;EACA;;EACAM;IACAC;MACAC;MACA;MACA;MACA;IACA;IACAC;MACAD;MACA;MACA;IACA;IACAE;MACA;QACAF;MACA;IACA;IACAG;MACA;QACAR;QACAC;QACAQ;UACAZ;QACA;MACA;QACAQ;QACA;UACA;UACA;QACA;UACAK;QACA;MAEA;IACA;EACA;EAEAC;IACA;EACA;AAEA","names":["name","data","tableData","pageSize","pageNum","total","methods","handleSizeChange","console","handleCurrentChange","loadGet","loadPost","param","alert","beforeMount"],"sourceRoot":"src/components","sources":["MainContent.vue"],"sourcesContent":["<template>\n  <div>\n    <div style=\"margin-bottom: 10px;text-align: center\">\n      <el-input v-model=\"name\" placeholder=\"Please enter name\" suffix-icon=\"el-icon-search\" style=\"width:200px\"\n                @keyup.enter.native=\"loadPost\"></el-input>\n      <el-button type=\"primary\" style=\"margin-left: 10px\" @click=\"loadPost\">Search</el-button>\n      <el-button type=\"success\">Reset</el-button>\n    </div>\n    <el-table :data=\"tableData\"\n        :header-cell-style=\"{ background:'#f2f5f2', color:'#555555'}\"\n              border\n    >\n      <el-table-column prop=\"id\" label=\"ID\" width=\"100\">\n      </el-table-column>\n      <el-table-column prop=\"no\" label=\"Account Number\" width=\"140\">\n      </el-table-column>\n      <el-table-column prop=\"name\" label=\"Name\" width=\"120\">\n      </el-table-column>\n      <el-table-column prop=\"age\" label=\"Age\" width=\"80\">\n      </el-table-column>\n      <el-table-column prop=\"gender\" label=\"Gender\" width=\"100\">\n        <template slot-scope=\"scope\">\n          <el-tag\n                :type=\"scope.row.gender===1?'primary':'success'\"\n                disable-transitions>{{scope.row.gender===1  ?'Male':'Female'}}\n          </el-tag>\n        </template>\n      </el-table-column>\n      <el-table-column prop=\"roleId\" label=\"Role\" width=\"120\">\n        <template slot-scope=\"scope\">\n          <el-tag\n              :type=\"scope.row.roleId===0?'danger':(scope.row.roleId===1?'primary':'success')\"\n              disable-transitions>{{scope.row.roleId===0?'super':\n                                    (scope.row.roleId===1?'manager':'general')}}\n          </el-tag>\n        </template>\n      </el-table-column>\n      <el-table-column prop=\"phone\" label=\"Phone Number\" width=\"160\">\n      </el-table-column>\n      <el-table-column prop=\"operate\" label=\"Operate\" width=\"200\">\n        <el-button size=\"mini\" type=\"success\"> Edit</el-button>\n        <el-button size=\"mini\" type=\"danger\"> Edit</el-button>\n      </el-table-column>\n    </el-table>\n    <el-pagination\n        @size-change=\"handleSizeChange\"\n        @current-change=\"handleCurrentChange\"\n        :current-page=\"pageNum\"\n        :page-sizes=\"[5, 10, 20,30]\"\n        :page-size=\"pageSize\"\n        layout=\"total, sizes, prev, pager, next, jumper\"\n        :total=\"total\">\n    </el-pagination>\n  </div>\n</template>\n\n\n<script>\nexport default {\n  name: \"MainContent\",\n  data() {\n    return{\n      tableData: [],\n      pageSize:10,\n      pageNum:1,\n      total:0,\n      data:'',\n      name:''//refer v-model里的name\n    }\n  },\n  methods: {\n    handleSizeChange(val) {\n      console.log(`${val} items per page`);\n      this.pageNum=1\n      this.pageSize=val\n      this.loadPost()\n    },\n    handleCurrentChange(val) {\n      console.log(`current page: ${val}`);\n      this.pageNum=val\n      this.loadPost()\n    },\n    loadGet() {\n      this.$axios.get(this.$httpUrl+'/user/list').then(res => res.data).then(res =>{\n        console.log(res)\n      })\n    },\n    loadPost() {\n      this.$axios.post(this.$httpUrl+'/user/listPageEn',{\n        pageSize: this.pageSize,\n        pageNum:this.pageNum,\n        param:{\n          name:this.name//refer data里的name\n        }\n      }).then(res=>res.data).then(res=>{\n        console.log(res)\n        if(res.code==200){\n          this.tableData=res.data\n          this.total=res.total\n        }else{\n          alert(\"Resource Get Failed\")\n        }\n\n      })\n    },\n  },\n\n  beforeMount() {\n    this.loadPost()\n  }\n\n}\n</script>\n\n<style scoped>\n\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}