n        </template>\n        <el-tag\n            type=\"success\"\n            disable-transitions>{{user.roleId==0?\"超级管理员\":(user.roleId==1?\"管理员\":\"用户\")}}</el-tag>\n\n      </el-descriptions-item>\n    </el-descriptions>\n\n    <DateUtils></DateUtils>\n  </div>\n</template>\n\n<script>\nimport DateUtils from \"./DateUtils\";\nexport default {\n  name: \"AccountCenter\",\n  components: {DateUtils},\n  data() {\n\n    return {\n      user:{}\n    }\n  },\n  computed:{\n\n  },\n  methods:{\n    init(){\n      this.user = JSON.parse(sessionStorage.getItem('CurUser'))\n      console.log(this.user)\n    }\n  },\n  created(){\n    console.log(\"11\")\n    this.init()\n  }\n}\n</script>\n\n<style scoped>\n.el-descriptions{\n  width:90%;\n\n  margin: 0 auto;\n  text-align: center;\n}\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}